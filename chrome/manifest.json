{
  "content_scripts": [
    {
      "js": [
        "inject.js"
      ],
      "matches": [
        "*://*.twitch.tv/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "description": "Twitch Mod Tools.",
  "icons": {
    "128": "assets/icon128.png",
    "48": "assets/icon48.png",
    "16": "assets/icon16.png"
  },
  "manifest_version": 2,
  "name": "Twitch Mod Tools",
  "permissions": [
    "*://*.twitch.tv/*",
    "*://*.rawgit.com/*"
  ],
  "version": "0.2"
}
