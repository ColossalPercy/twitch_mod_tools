{
  "content_scripts": [
    {
      "js": [
        "inject.js"
      ],
      "matches": [
        "*://*.twitch.tv/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "description": "Add useful utilities to twitch.tv!",
  "icons": {
    "128": "assets/icon128.png",
    "48": "assets/icon48.png",
    "16": "assets/icon16.png"
  },
  "manifest_version": 2,
  "name": "Twitch Utils",
  "permissions": [
    "*://*.twitch.tv/*",
    "*://*.rawgit.com/*"
  ],
  "version": "0.4"
}
